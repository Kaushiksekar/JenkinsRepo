def lib = library('my-shared-library@master').com.trial.jenkins.stage;

node("master"){
    stage ('Stage 1'){
    	steps{
    		println("starting stage1")
    		script{
		    	println(lib.Stage.getName("Stage1"));
    		}
    	}
    }
    stage ('Stage 2'){
    	steps{
    		println("starting stage2")
    		script{
		    	println(lib.Stage.getName("Stage1"));
    		}
    	}
    }
}
