library('my-shared-library').com.trial.jenkins.stage.Stage.getName;

pipeline{
	agent any
    stage ('Stage 1'){
    	steps{
    		println("starting stage1")
    		script{
		    	println(getName("Stage1"));
    		}
    	}
    }
    stage ('Stage 2'){
    	steps{
    		println("starting stage2")
    		script{
		    	println(getName("Stage2"));
    		}
    	}
    }
}
