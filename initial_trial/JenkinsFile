@Library('my-shared-library@master') _
import com.trial.jenkins.stage.Stage;

node("master"){
    stage ('Stage 1'){
    	steps{
    		println("starting stage1")
    		script{
		    	println(Stage.getName("Stage1"));
    		}
    	}
    }
    stage ('Stage 2'){
    	steps{
    		println("starting stage2")
    		script{
		    	println(Stage.getName("Stage1"));
    		}
    	}
    }
}
